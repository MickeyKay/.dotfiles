#
# Directory Navigation Aliases
#

# Pull out!
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# DOT files
alias dotd="cd ~/.dotfiles"

# GRUNT files
alias gruntd="cd ~/.grunt-init"

# Base directories
alias root="cd /"
alias home="cd ~/"
alias desk="cd ~/Desktop"
alias sites="cd ~/Sites"

alias subUser="cd ~/Library/Application\ Support/Sublime\ Text\ 3/Packages/User"