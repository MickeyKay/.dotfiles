#
# Directory Navigation Aliases
#

# Pull out!
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# DOT files
alias dots="cd ~/.dotfiles"

# grunt-init 
alias gi="cd ~/.grunt-init"

# Base directories
alias root="cd /"
alias home="cd ~/"
alias desk="cd ~/Desktop"
alias sites="cd ~/Sites"